{
  "name": "Excalibur",
  "version": "1.0.0",
  "scripts": {
    "start": "yarn run build && yarn run watch",
    "build": "yarn run build-ts",
    "serve": "node dist/server/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn run watch-ts\" \"nodemon dist/server/server.js\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w"
  },
  "repository": "https://github.com/kostkams/Excalibur.git",
  "author": "Matthias Kostka <matthias@kostkams.de>",
  "license": "GNU GPLv3",
  "private": true,
  "dependencies": {
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "inversify": "^4.11.1",
    "inversify-express-utils": "^5.2.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/react-redux": "^5.0.15",
    "@types/redux-immutable-state-invariant": "^2.0.4",
    "awesome-typescript-loader": "^4.0.1",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^3.5.1",
    "gulp": "^3.9.1",
    "gulp-livereload": "^3.8.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-notify": "^3.2.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "source-map-loader": "^0.2.3",
    "ts-loader": "^4.1.0",
    "typescript": "^2.7.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-middleware": "^3.0.1",
    "webpack-dev-server": "^3.1.1"
  }
}
